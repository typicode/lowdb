{
  "name": "lowdb",
  "version": "1.0.0",
  "main": "lib/index.js",
  "scripts": {
    "test": "jest",
    "lint": "tslint 'src/**/*.ts'",
    "fix": "npm run lint -- --fix",
    "toc": "doctoc README.md",
    "build": "del-cli lib && tsc",
    "prepublishOnly": "npm run build && node test/lib && pkg-ok"
  },
  "repository": "https://github.com/typicode/lowdb",
  "author": "typicode <typicode@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^23.3.1",
    "@types/lodash": "^4.14.116",
    "@types/node": "^10.9.3",
    "@types/tempy": "^0.1.0",
    "@types/write-file-atomic": "^2.1.1",
    "del-cli": "^1.1.0",
    "doctoc": "^1.3.1",
    "husky": "^1.1.0",
    "jest": "^23.5.0",
    "jest-localstorage-mock": "^2.2.0",
    "lodash": "^4.17.10",
    "prettier": "^1.14.2",
    "tempy": "^0.2.1",
    "ts-jest": "^23.1.4",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-plugin-prettier": "^1.3.0",
    "typescript": "^3.0.3"
  },
  "dependencies": {
    "mutexify": "^1.2.0",
    "write-file-atomic": "^2.3.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test",
      "post-commit": "echo post-commit"
    }
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
